{
  "name": "Armor-lux Marini√®re",
  "type": "light",
  "colors": {
    "editor.background": "#FFFFFF",
    "editor.foreground": "#333333",
    "editorLineNumber.foreground": "#0047AB",
    "editorCursor.foreground": "#0047AB",
    "activityBar.background": "#0047AB",
    "activityBar.foreground": "#FFFFFF",
    "activityBarBadge.background": "#FFFFFF",
    "activityBarBadge.foreground": "#0047AB",
    "sideBar.background": "#F8F8F8",
    "sideBar.foreground": "#333333",
    "sideBarSectionHeader.background": "#EAEAEA",
    "sideBarSectionHeader.foreground": "#0047AB",
    "statusBar.background": "#0047AB",
    "statusBar.foreground": "#FFFFFF",
    "statusBar.noFolderBackground": "#003366",
    "statusBarItem.remoteBackground": "#FFFFFF",
    "statusBarItem.remoteForeground": "#0047AB",
    "tab.activeBackground": "#FFFFFF",
    "tab.activeForeground": "#0047AB",
    "tab.inactiveBackground": "#EAEAEA",
    "tab.inactiveForeground": "#555555",
    "tab.border": "#EAEAEA",
    "titleBar.activeForeground": "#FFFFFF",
    "titleBar.inactiveBackground": "#005EB8",
    "titleBar.inactiveForeground": "#EAEAEA",
    "button.background": "#0047AB",
    "button.foreground": "#FFFFFF",
    "input.background": "#FFFFFF",
    "input.border": "#0047AB"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#888888",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#006400"
      }
    },
    {
      "scope": ["variable", "support.variable", "punctuation.definition.variable"],
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#B85B00"
      }
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#0047AB"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.class", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#0047AB",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#003366"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#8B4513"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF0000",
        "background": "#FFDDDD"
      }
    }
  ]
}

