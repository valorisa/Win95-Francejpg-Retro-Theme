{
  "name": "chaudard-ail-theme",
  "displayName": "Chaudard - Ail Edition",
  "description": "Thème VSCode inspiré du packaging 'On veut de l'ail!' avec l'Adjudant-Chef Chaudard.",
  "version": "1.0.0",
  "publisher": "ton-nom-ou-pseudo",
  "type": "light",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Themes"
  ],
  "colors": {
    "editor.background": "#FDF5D5",
    "editor.foreground": "#3B2B1A",
    "editorLineNumber.foreground": "#A0804E",
    "editorCursor.foreground": "#D81E05",

    "activityBar.background": "#FCD647",
    "activityBar.foreground": "#3B2B1A",
    "activityBar.inactiveForeground": "#8A785D",
    "activityBarBadge.background": "#D81E05",
    "activityBarBadge.foreground": "#FFFFFF",

    "sideBar.background": "#FAEBBF",
    "sideBar.foreground": "#3B2B1A",
    "sideBarSectionHeader.background": "#FCD647",
    "sideBarSectionHeader.foreground": "#3B2B1A",

   
    "statusBar.background": "#D81E05",
    "statusBar.noFolderBackground": "#A31604",
    "statusBarItem.remoteBackground": "#FFFFFF",
    "statusBarItem.remoteForeground": "#D81E05",

   
    "tab.activeBackground": "#FDF5D5",
    "tab.inactiveForeground": "#3B2B1A",
    "tab.border": "#EAC985",

    "titleBar.activeBackground": "#D81E05",
    "titleBar.activeForeground": "#FFFFFF",
    
    "button.background": "#D81E05",
    "button.foreground": "#FFFFFF",
    "input.background": "#FFFFFF",
    "dropdown.border": "#D81E05",
    "list.hoverBackground": "#FCE48B",
    "list.activeSelectionBackground": "#D81E05",
    "list.activeSelectionForeground": "#FFFFFF"
  },
  "tokenColors": [
  
    {
      "scope": ["comment", "comment.block.documentation"],
      "settings": {
        "foreground": "#8A785D",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "variable.language"],
      "settings": {
        "foreground": "#D81E05",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["constant", "constant.other"],
      "settings": {
        "foreground": "#A0804E"
      }
    },
    {
      "scope": ["meta", "meta.definition"],
      "settings": {
        "foreground": "#3B2B1A"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#D81E05",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#8A785D"
      }
    },
    {
      "scope": ["keyword", "variable.language"],
      "settings": {
        "foreground": "#D81E05",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["constant", "constant.other"],
      "settings": {
        "foreground": "#A0804E"
      }
    },
    {
      "scope": ["meta", "meta.definition"],
      "settings": {
        "foreground": "#3B2B1A"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#D81E05",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#008847"
      }
    },
    {
      "scope": ["variable", "support.variable", "punctuation.definition.variable"],
      "settings": {
        "foreground": "#3B2B1A"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#EF7D00"
      }
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#4A4A4A",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.class", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#3B2B1A",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#D81E05"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#A0804E"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF0000",
        "background": "#FFDDDD"
      }
    }
  ]
}


